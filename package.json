  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev": "concurrently \"npm start\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack  --env mode=production apiUrl=https://productionserver1-x1ri9s3w.b4a.run/",
    "build:dev": "webpack  --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test --config .lokirc.json",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference update --reactUri file:./storybook-static",
    "test:ui:report": "reg-cli ./.loki/current ./.loki/reference ./.loki/difference -R ./.loki/report.html && move reg.json .\\.loki\\reg.json",
    "test:ui:update": "loki update --reactUri file:./storybook-static",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build --test -c ./config/storybook",
    "prepare": "husky install"
  },
